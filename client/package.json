{
  "name": "client-web",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:mocha": "cross-env NODE_ENV=test mocha --exit --require @babel/register --require src/mochaSetup.js \"src/**/*.mocha.{js,jsx}\"",
    "test:mocha:coverage": "cross-env NODE_ENV=test nyc mocha --exit --require src/mochaSetup.js \"src/**/*.mocha.{js,jsx}\""
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "axios": "^0.19.0",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-react-app": "9.0.0",
    "cross-env": "^5.2.0",
    "expect": "^24.8.0",
    "http-proxy-middleware": "^0.19.1",
    "jest-dom": "^3.4.0",
    "jsdom": "^15.1.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.11",
    "mocha": "^6.1.3",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "nyc": "^14.1.1",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.8.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "sinon": "^7.3.1",
    "yup": "^0.27.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          "rewire"
        ]
      }
    },
    "presets": [
      "react-app"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**/*.mocha.{js,jsx}",
      "src/mochaSetup.js"
    ],
    "extends": "@istanbuljs/nyc-config-babel",
    "extension": [
      ".js",
      ".jsx"
    ],
    "include": "src/**/*"
  }
}
