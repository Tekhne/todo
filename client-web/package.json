{
  "name": "client-web",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:mocha": "cross-env NODE_ENV=test mocha --exit --require @babel/register --require src/mochaSetup.js \"src/**/*.mocha.{js,jsx}\"",
    "test:mocha:coverage": "cross-env NODE_ENV=test nyc --require @babel/register mocha --exit --require src/mochaSetup.js \"src/**/*.mocha.{js,jsx}\""
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/register": "^7.4.0",
    "axios": "^0.18.0",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-preset-react-app": "^7.0.2",
    "cross-env": "^5.2.0",
    "dom-testing-library": "^3.19.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "expect": "^24.7.1",
    "jest-dom": "^3.1.3",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.11",
    "mocha": "^6.1.3",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "nyc": "^13.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "sinon": "^7.3.1",
    "yup": "^0.27.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "presets": [
      "react-app"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**/*.mocha.{js,jsx}",
      "src/mochaSetup.js"
    ]
  }
}
